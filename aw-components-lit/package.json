{
  "name": "aw-components-lit",
  "version": "0.0.1",
  "description": "Lit Component Library - Clone of aw-components using Lit framework",
  "main": "dist/aw-components.umd.js",
  "module": "dist/aw-components.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:install": "playwright install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "lit",
    "web-components",
    "components",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@motionone/dom": "^10.16.4",
    "lit": "^3.3.1",
    "three": "^0.167.1"
  },
  "devDependencies": {
    "@lit/localize-tools": "^0.8.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/web-components": "^8.6.14",
    "@storybook/web-components-vite": "^8.6.14",
    "@types/node": "^24.3.0",
    "storybook": "^8.6.14",
    "typescript": "^5.9.2",
    "vite": "^4.5.0",
    "web-dev-server": "^3.0.27"
  }
}
